	<!-- #### DELIBERATIVE AGENTS ### -->
	<agent name="deliberative-naive">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		
	</agent>
	
	<agent name="deliberative-BFS">
		<set class-path="bin/"/>
